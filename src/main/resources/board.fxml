<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="Board"
      style="-fx-background-color: #B87333"
      fx:id="layout"
      prefHeight="600.0" prefWidth="600.0">
    <HBox>
        <TextField fx:id="status" editable="false" prefWidth="300" />
        <Button fx:id="skip" text="Skip" minWidth="100" onAction="#skipHandler"/>
    </HBox>
    <Pane fx:id="pane" prefWidth="560" prefHeight="720" style="-fx-background-color: #B87333"  />


</VBox>